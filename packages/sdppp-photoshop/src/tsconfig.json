{
    "extends": "../../../tsconfig.base.json",
    "compilerOptions": {
        "erasableSyntaxOnly": true,
        "jsx": "preserve",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "ES2022",
            "DOM"
        ],
        "allowImportingTsExtensions": true,
        "allowJs": true,
        "noEmit": true,
        "typeRoots": [
            "../node_modules/@types"
        ]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ]
}